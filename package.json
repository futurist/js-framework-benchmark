{
  "name": "js-framework-benchmark",
  "version": "1.0.0",
  "description": "Simple Benchmark for Javascript client side rendering",
  "scripts": {
    "build": "node install.js && node build.js",
    "build-prod": "node install.js && node build.js",
    "compile": "node install.js && node build.js",
    "start": "http-server -c-1 . -p 9999",
    "selenium": "cd webdriver-ts && npm run selenium && npm run results",
    "check": "cd webdriver-ts && npm run selenium -- --count 1 --exitOnError true"
  },
  "keywords": [
    "benchmark",
    "javascript",
    "frontend",
    "client",
    "react",
    "angular",
    "mithril"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/krausest/js-framework-benchmark.git"
  },
  "author": "Stefan Krause",
  "license": "ISC",
  "homepage": "https://github.com/krausest/js-framework-benchmark",
  "dependencies": {
    "command-exists": "^1.0.2",
    "fs-extra": "^0.30.0",
    "http-server": "^0.9.0",
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "yargs": "^6.6.0"
  }
}
